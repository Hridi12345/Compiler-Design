#include<iostream>
using namespace std;

int main() {
    string input;
    cout << "Enter input: ";

    cin >> input;
    int i = 0, flag = 1;

    while (input[i] != '\0') {
        if (input[i] >= '0' && input[i] <= '9') {

        } else {
           flag = 0;
            break;
        }
        i++;
    }

    if (flag == 1) {
        cout << "Numeric constant" << endl;
    } else {
        cout << "Not numeric" << endl;
    }

    return 0;
}


